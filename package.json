{
  "name": "mini-pg-migrate",
  "version": "0.0.1",
  "keywords": [
    "pg",
    "node-postgres",
    "migrate",
    "migration"
  ],
  "license": "MIT",
  "description": "A minimal postgres db migration",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vitest dev",
    "prepack": "pnpm run build",
    "db:generate": "npx ts-node src/index.ts generate",
    "db:migrate:up": "npx ts-node src/index.ts migrate:up",
    "db:migrate:down": "npx ts-node src/index.ts migrate:down"
  },
  "dependencies": {
    "c12": "^1.11.1",
    "citty": "^0.1.6",
    "pg": "^8.12.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.11",
    "@types/pg": "^8.11.6",
    "typescript": "^5.5.3",
    "unbuild": "^2.0.0",
    "vitest": "^2.0.3"
  },
  "packageManager": "pnpm@9.6.0"
}
